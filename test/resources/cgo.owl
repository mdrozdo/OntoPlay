<rdf:RDF xmlns="http://purl.org/NET/cgo#"
     xml:base="http://purl.org/NET/cgo"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:papers="http://www.cs.man.ac.uk/~rizos/papers/"
     xmlns:cgo="http://purl.org/NET/cgo#">
    <owl:Ontology rdf:about="http://purl.org/NET/cgo">
        <rdfs:label xml:lang="en">Core Grid Ontology</rdfs:label>
        <rdfs:label xml:lang="en">cgo</rdfs:label>
        <owl:versionInfo xml:lang="en">After reengineering number 4 (19.11.2013)</owl:versionInfo>
        <owl:versionInfo xml:lang="en">Branch of the Core Grid Ontology started in 06.2013 for the purpose of Agents in Grid project</owl:versionInfo>
        <rdfs:comment xml:lang="en">The Core Grid Ontology is designed to represent the knowledge of grid systems</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&papers;ccgrid06.pdf"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/cgo#InstalledSoftware -->

    <owl:ObjectProperty rdf:about="&cgo;InstalledSoftware"/>
    


    <!-- http://purl.org/NET/cgo#RAMSize -->

    <owl:ObjectProperty rdf:about="&cgo;RAMSize"/>
    


    <!-- http://purl.org/NET/cgo#accessControlBaseType -->

    <owl:ObjectProperty rdf:about="&cgo;accessControlBaseType"/>
    


    <!-- http://purl.org/NET/cgo#available -->

    <owl:ObjectProperty rdf:about="&cgo;available"/>
    


    <!-- http://purl.org/NET/cgo#belongToVO -->

    <owl:ObjectProperty rdf:about="&cgo;belongToVO">
        <rdfs:comment xml:lang="en">Any Grid entities must belong to a VO.</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;GridEntity"/>
        <rdfs:range rdf:resource="&cgo;VO"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#creationDate -->

    <owl:ObjectProperty rdf:about="&cgo;creationDate"/>
    


    <!-- http://purl.org/NET/cgo#dependency -->

    <owl:ObjectProperty rdf:about="&cgo;dependency"/>
    


    <!-- http://purl.org/NET/cgo#descriptor -->

    <owl:ObjectProperty rdf:about="&cgo;descriptor">
        <rdfs:comment xml:lang="en">The description of a Grid service. Here means a file in Web Service Description Language.</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;GridService"/>
        <rdfs:range rdf:resource="&cgo;WDSL"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#estimatedResponseTime -->

    <owl:ObjectProperty rdf:about="&cgo;estimatedResponseTime"/>
    


    <!-- http://purl.org/NET/cgo#fileSystem -->

    <owl:ObjectProperty rdf:about="&cgo;fileSystem"/>
    


    <!-- http://purl.org/NET/cgo#globalOwner -->

    <owl:ObjectProperty rdf:about="&cgo;globalOwner"/>
    


    <!-- http://purl.org/NET/cgo#gridEntry -->

    <owl:ObjectProperty rdf:about="&cgo;gridEntry">
        <rdfs:comment xml:lang="en">An entry for a user to a Grid system.</rdfs:comment>
        <rdfs:range rdf:resource="&cgo;UserInterface"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasCPU -->

    <owl:ObjectProperty rdf:about="&cgo;hasCPU">
        <rdfs:comment xml:lang="en">The computing power.</rdfs:comment>
        <rdfs:range rdf:resource="&cgo;CPU"/>
        <rdfs:domain rdf:resource="&cgo;ComputingComponent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasComponent -->

    <owl:ObjectProperty rdf:about="&cgo;hasComponent">
        <rdfs:comment xml:lang="en">A Grid has different components.</rdfs:comment>
        <rdfs:range rdf:resource="&cgo;GridComponent"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cgo;GridMiddleware"/>
                    <rdf:Description rdf:about="&cgo;Site"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasExe -->

    <owl:ObjectProperty rdf:about="&cgo;hasExe"/>
    


    <!-- http://purl.org/NET/cgo#hasID -->

    <owl:ObjectProperty rdf:about="&cgo;hasID">
        <rdfs:comment xml:lang="en">An identifier of a Grid entity.</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;GridEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasIP -->

    <owl:ObjectProperty rdf:about="&cgo;hasIP">
        <rdfs:range rdf:resource="&cgo;IP"/>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasID"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cgo;GridComponent"/>
                    <rdf:Description rdf:about="&cgo;NetworkResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasMAC -->

    <owl:ObjectProperty rdf:about="&cgo;hasMAC">
        <rdfs:range rdf:resource="&cgo;MAC"/>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasID"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cgo;GridComponent"/>
                    <rdf:Description rdf:about="&cgo;NetworkResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasOperatingSystem -->

    <owl:ObjectProperty rdf:about="&cgo;hasOperatingSystem"/>
    


    <!-- http://purl.org/NET/cgo#hasPolicy -->

    <owl:ObjectProperty rdf:about="&cgo;hasPolicy">
        <rdfs:comment xml:lang="en">The policy statement for Grid VOs.</rdfs:comment>
        <rdfs:range rdf:resource="&cgo;Policy"/>
        <rdfs:domain rdf:resource="&cgo;VO"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasPort -->

    <owl:ObjectProperty rdf:about="&cgo;hasPort">
        <rdfs:comment xml:lang="en">The port number of a Grid service.</rdfs:comment>
        <rdfs:range rdf:resource="&cgo;Port"/>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasID"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cgo;GridComponent"/>
                    <rdf:Description rdf:about="&cgo;GridService"/>
                    <rdf:Description rdf:about="&cgo;NetworkResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasQueue -->

    <owl:ObjectProperty rdf:about="&cgo;hasQueue">
        <rdfs:range rdf:resource="&cgo;Queue"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cgo;ComputingComponent"/>
                    <rdf:Description rdf:about="&cgo;JobScheduler"/>
                    <rdf:Description rdf:about="&cgo;Jobmanager"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasURI -->

    <owl:ObjectProperty rdf:about="&cgo;hasURI">
        <rdfs:range rdf:resource="&cgo;URI"/>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasID"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cgo;DataResource"/>
                    <rdf:Description rdf:about="&cgo;GridComponent"/>
                    <rdf:Description rdf:about="&cgo;GridService"/>
                    <rdf:Description rdf:about="&cgo;NetworkResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#hasWN -->

    <owl:ObjectProperty rdf:about="&cgo;hasWN">
        <rdfs:comment xml:lang="en">Each ComputingElement should have several WorkNodes.</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;ComputingElement"/>
        <rdfs:range rdf:resource="&cgo;WorkerNode"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#localOwner -->

    <owl:ObjectProperty rdf:about="&cgo;localOwner"/>
    


    <!-- http://purl.org/NET/cgo#maxRunningJobs -->

    <owl:ObjectProperty rdf:about="&cgo;maxRunningJobs">
        <rdfs:domain rdf:resource="&cgo;ComputingComponent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#maxTotalJobs -->

    <owl:ObjectProperty rdf:about="&cgo;maxTotalJobs"/>
    


    <!-- http://purl.org/NET/cgo#maxWallClockTime -->

    <owl:ObjectProperty rdf:about="&cgo;maxWallClockTime"/>
    


    <!-- http://purl.org/NET/cgo#model -->

    <owl:ObjectProperty rdf:about="&cgo;model"/>
    


    <!-- http://purl.org/NET/cgo#needLib -->

    <owl:ObjectProperty rdf:about="&cgo;needLib">
        <rdfs:comment xml:lang="en">Grid application need libiary to run.</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;GridApplication"/>
        <rdfs:range rdf:resource="&cgo;Lib"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/cgo#networkAdapterType -->

    <owl:ObjectProperty rdf:about="&cgo;networkAdapterType"/>
    


    <!-- http://purl.org/NET/cgo#onHost -->

    <owl:ObjectProperty rdf:about="&cgo;onHost"/>
    


    <!-- http://purl.org/NET/cgo#platformType -->

    <owl:ObjectProperty rdf:about="&cgo;platformType"/>
    


    <!-- http://purl.org/NET/cgo#policyType -->

    <owl:ObjectProperty rdf:about="&cgo;policyType"/>
    


    <!-- http://purl.org/NET/cgo#priority -->

    <owl:ObjectProperty rdf:about="&cgo;priority"/>
    


    <!-- http://purl.org/NET/cgo#processor -->

    <owl:ObjectProperty rdf:about="&cgo;processor"/>
    


    <!-- http://purl.org/NET/cgo#programmeLang -->

    <owl:ObjectProperty rdf:about="&cgo;programmeLang"/>
    


    <!-- http://purl.org/NET/cgo#readOnly -->

    <owl:ObjectProperty rdf:about="&cgo;readOnly"/>
    


    <!-- http://purl.org/NET/cgo#registeredUser -->

    <owl:ObjectProperty rdf:about="&cgo;registeredUser">
        <rdfs:comment xml:lang="en">The users who registered to a VO.</rdfs:comment>
        <rdfs:range rdf:resource="&cgo;GridUser"/>
        <rdfs:domain rdf:resource="&cgo;VO"/>
    </owl:ObjectProperty>
    


