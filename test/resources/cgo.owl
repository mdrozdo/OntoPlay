    <!-- http://purl.org/NET/cgo#availableNum -->

    <owl:DatatypeProperty rdf:about="&cgo;availableNum">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;int"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cgo;CPU"/>
                    <rdf:Description rdf:about="&cgo;StorageSpace"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#freeCPUs -->

    <owl:DatatypeProperty rdf:about="&cgo;freeCPUs">
        <rdfs:domain rdf:resource="&cgo;ComputingComponent"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasClockSpeed -->

    <owl:DatatypeProperty rdf:about="&cgo;hasClockSpeed"/>
    


    <!-- http://purl.org/NET/cgo#hasIDData -->

    <owl:DatatypeProperty rdf:about="&cgo;hasIDData">
        <rdfs:label xml:lang="en">has ID</rdfs:label>
        <rdfs:comment xml:lang="en">An identifier of a grid entity</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;GridEntity"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasIPAddress -->

    <owl:DatatypeProperty rdf:about="&cgo;hasIPAddress">
        <rdfs:label xml:lang="en">has IP</rdfs:label>
        <rdfs:comment xml:lang="en">Internet protocol address</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;IP"/>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasIDData"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasIPv4 -->

    <owl:DatatypeProperty rdf:about="&cgo;hasIPv4">
        <rdfs:label xml:lang="en">has IP version 4</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasIPAddress"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasIPv6 -->

    <owl:DatatypeProperty rdf:about="&cgo;hasIPv6">
        <rdfs:label xml:lang="en">has IP version 6</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasIPAddress"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasMACAddress -->

    <owl:DatatypeProperty rdf:about="&cgo;hasMACAddress">
        <rdfs:label xml:lang="en">has MAC</rdfs:label>
        <rdfs:comment xml:lang="en">Media access control address</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;MAC"/>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasIDData"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasModelName -->

    <owl:DatatypeProperty rdf:about="&cgo;hasModelName">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasName -->

    <owl:DatatypeProperty rdf:about="&cgo;hasName">
        <rdfs:comment xml:lang="en">A hunman-readable name of a Grid entity</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasPortNumber -->

    <owl:DatatypeProperty rdf:about="&cgo;hasPortNumber">
        <rdfs:domain rdf:resource="&cgo;Port"/>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasIDData"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:maxExclusive rdf:datatype="&xsd;integer">65536</xsd:maxExclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="&xsd;integer">0</xsd:minExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasURIAddress -->

    <owl:DatatypeProperty rdf:about="&cgo;hasURIAddress">
        <rdfs:label xml:lang="en">has Uniform Resource Identifier</rdfs:label>
        <rdfs:domain rdf:resource="&cgo;URL"/>
        <rdfs:subPropertyOf rdf:resource="&cgo;hasIDData"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#hasVendor -->

    <owl:DatatypeProperty rdf:about="&cgo;hasVendor">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#location -->

    <owl:DatatypeProperty rdf:about="&cgo;location">
        <rdfs:comment xml:lang="en">Geographical location</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;Site"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#mainMemory -->

    <owl:DatatypeProperty rdf:about="&cgo;mainMemory">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#maxCPUTime -->

    <owl:DatatypeProperty rdf:about="&cgo;maxCPUTime">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#releaseVersion -->

    <owl:DatatypeProperty rdf:about="&cgo;releaseVersion">
        <rdfs:comment xml:lang="en">The realeased version of a Grid middleware.</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;GridMiddleware"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#runningJobs -->

    <owl:DatatypeProperty rdf:about="&cgo;runningJobs">
        <rdfs:domain rdf:resource="&cgo;ComputingComponent"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#status -->

    <owl:DatatypeProperty rdf:about="&cgo;status">
        <rdfs:comment xml:lang="en">The state of a Grid service.</rdfs:comment>
        <rdfs:domain rdf:resource="&cgo;GridService"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#totalJobs -->

    <owl:DatatypeProperty rdf:about="&cgo;totalJobs">
        <rdfs:domain rdf:resource="&cgo;ComputingComponent"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#waitingJobs -->

    <owl:DatatypeProperty rdf:about="&cgo;waitingJobs">
        <rdfs:domain rdf:resource="&cgo;ComputingComponent"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/cgo#worstResponseTime -->

    <owl:DatatypeProperty rdf:about="&cgo;worstResponseTime">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


