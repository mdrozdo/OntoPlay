    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/cgo#AccessControl -->

    <owl:Class rdf:about="&cgo;AccessControl">
        <rdfs:subClassOf rdf:resource="&cgo;Policy"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#Accounting -->

    <owl:Class rdf:about="&cgo;Accounting">
        <rdfs:subClassOf rdf:resource="&cgo;GridService"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#ApplicationEnv -->

    <owl:Class rdf:about="&cgo;ApplicationEnv">
        <rdfs:subClassOf rdf:resource="&cgo;Software"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#AuthenticationS -->

    <owl:Class rdf:about="&cgo;AuthenticationS">
        <rdfs:subClassOf rdf:resource="&cgo;Security"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#AuthorizationS -->

    <owl:Class rdf:about="&cgo;AuthorizationS">
        <rdfs:subClassOf rdf:resource="&cgo;Security"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#BDII -->

    <owl:Class rdf:about="&cgo;BDII">
        <rdfs:subClassOf rdf:resource="&cgo;GridInformationService"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#Bandwidth -->

    <owl:Class rdf:about="&cgo;Bandwidth">
        <rdfs:subClassOf rdf:resource="&cgo;NetworkResource"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#CPU -->

    <owl:Class rdf:about="&cgo;CPU">
        <rdfs:subClassOf rdf:resource="&cgo;ComputingResource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&cgo;hasVendor"/>
                <owl:allValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&cgo;hasClockSpeed"/>
                <owl:someValuesFrom rdf:resource="&xsd;int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&cgo;hasModelName"/>
                <owl:allValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This class describes models of CPUs, rather than individual CPUs.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#Cert -->

    <owl:Class rdf:about="&cgo;Cert">
        <rdfs:subClassOf rdf:resource="&cgo;Policy"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#Cluster -->

    <owl:Class rdf:about="&cgo;Cluster">
        <rdfs:subClassOf rdf:resource="&cgo;ComputingResource"/>
        <rdfs:subClassOf rdf:resource="&cgo;Hardware"/>
    </owl:Class>
    


    <!-- http://purl.org/NET/cgo#CoAllocator -->

    <owl:Class rdf:about="&cgo;CoAllocator">
        <rdfs:subClassOf rdf:resource="&cgo;GridComponent"/>
