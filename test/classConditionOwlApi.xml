<?xml version="1.0"?>

<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:AiGGridOntology="http://gridagents.sourceforge.net/AiGGridOntology#"
     xmlns:ont="http://www.co-ode.org/ontologies/ont.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:cgo="http://purl.org/NET/cgo#"
     xmlns:bla="http://bla.org/">
    

   <Class rdf:about="http://bla.org/testCondition">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/NET/cgo#WorkerNode"/>
                    <Restriction>
                        <onProperty rdf:resource="http://gridagents.sourceforge.net/AiGGridOntology#hasMemory"/>
                        <someValuesFrom>
                            <Class>
                                <intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://purl.org/NET/cgo#Memory"/>
                                    <Restriction>
                                        <onProperty rdf:resource="http://gridagents.sourceforge.net/AiGGridOntology#hasTotalSize"/>
                                        <hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#float">123.0</hasValue>
                                    </Restriction>
                                </intersectionOf>
                            </Class>
                        </someValuesFrom>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
    </Class>
    
</rdf:RDF>
