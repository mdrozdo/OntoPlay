<?xml version="1.0"?>
<rdf:RDF xmlns="http://drozdowicz.net/onto/fitness-tracking#"
     xml:base="http://drozdowicz.net/onto/fitness-tracking"
     xmlns:schema="http://schema.org/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:wgs84_pos="http://www.w3.org/2003/01/geo/wgs84_pos#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://drozdowicz.net/onto/fitness-tracking">
        <owl:imports rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:comment xml:lang="en">A sample fitness data tracking ontology, used as an element of a use case for semantic data and privacy control.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#alt -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#alt">
        <rdfs:comment>The WGS84 altitude of a SpatialThing (decimal meters 
above the local reference ellipsoid).</rdfs:comment>
        <rdfs:label>altitude</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#lat -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#lat">
        <rdfs:comment>The WGS84 latitude of a SpatialThing (decimal degrees).</rdfs:comment>
        <rdfs:label>latitude</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#location -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#location">
        <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/based_near"/>
        <rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#long -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#long">
        <rdfs:comment>The WGS84 longitude of a SpatialThing (decimal degrees).</rdfs:comment>
        <rdfs:label>longitude</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
    </owl:AnnotationProperty>
    


    <!-- http://xmlns.com/foaf/0.1/based_near -->

    <owl:AnnotationProperty rdf:about="http://xmlns.com/foaf/0.1/based_near"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://drozdowicz.net/onto/fitness-tracking#aggregatesMetric -->

    <owl:ObjectProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#aggregatesMetric">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#AggregateMetric"/>
        <rdfs:range rdf:resource="http://drozdowicz.net/onto/fitness-tracking#TrainingMetric"/>
    </owl:ObjectProperty>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#describedWithMetric -->

    <owl:ObjectProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#describedWithMetric">
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Training"/>
        <rdfs:range rdf:resource="http://drozdowicz.net/onto/fitness-tracking#TrainingMetric"/>
    </owl:ObjectProperty>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#distance -->

    <owl:ObjectProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#distance">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Route"/>
        <rdfs:range rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Distance"/>
    </owl:ObjectProperty>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#includesLocation -->

    <owl:ObjectProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#includesLocation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#partOfRoute"/>
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Route"/>
        <rdfs:range rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Location"/>
    </owl:ObjectProperty>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#measuredDuring -->

    <owl:ObjectProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#measuredDuring">
        <owl:inverseOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#measuredHeartRate"/>
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#HeartRate"/>
        <rdfs:range rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Training"/>
    </owl:ObjectProperty>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#measuredHeartRate -->

    <owl:ObjectProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#measuredHeartRate">
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Training"/>
        <rdfs:range rdf:resource="http://drozdowicz.net/onto/fitness-tracking#HeartRate"/>
    </owl:ObjectProperty>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#partOfRoute -->

    <owl:ObjectProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#partOfRoute">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Location"/>
        <rdfs:range rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Route"/>
    </owl:ObjectProperty>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#relatedToTraining -->

    <owl:ObjectProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#relatedToTraining">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#GeospatialMeasurement"/>
        <rdfs:range rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Training"/>
    </owl:ObjectProperty>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#trainingLocation -->

    <owl:ObjectProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#trainingLocation">
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Training"/>
        <rdfs:range rdf:resource="http://drozdowicz.net/onto/fitness-tracking#GeospatialMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#location -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#location"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://drozdowicz.net/onto/fitness-tracking#aggregationDays -->

    <owl:DatatypeProperty rdf:about="http://drozdowicz.net/onto/fitness-tracking#aggregationDays">
        <rdfs:domain rdf:resource="http://drozdowicz.net/onto/fitness-tracking#AggregateMetric"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://drozdowicz.net/onto/access-control#Sensitivity -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/access-control#Sensitivity">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#AggregateMetric -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#AggregateMetric">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Metric"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#BloodPressure -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#BloodPressure">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#HealthMeasurement"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#CaloriesBurned -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#CaloriesBurned">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#TrainingMetric"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#CardiovascularTraining -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#CardiovascularTraining">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Training"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Cycling -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Cycling">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#CardiovascularTraining"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Distance -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Distance">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#TrainingMetric"/>
        <owl:disjointWith rdf:resource="http://drozdowicz.net/onto/fitness-tracking#StepCount"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#GeospatialMeasurement -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#GeospatialMeasurement"/>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#HealthMeasurement -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#HealthMeasurement">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#HeartRate -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#HeartRate">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#HealthMeasurement"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Height -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Height">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#HealthMeasurement"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#IndoorCycling -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#IndoorCycling">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Cycling"/>
        <owl:disjointWith rdf:resource="http://drozdowicz.net/onto/fitness-tracking#OutdoorCycling"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#IndoorRunning -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#IndoorRunning">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Running"/>
        <owl:disjointWith rdf:resource="http://drozdowicz.net/onto/fitness-tracking#OutdoorRunning"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#IndoorTraining -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#IndoorTraining">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Training"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#IndoorWalking -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#IndoorWalking">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Walking"/>
        <owl:disjointWith rdf:resource="http://drozdowicz.net/onto/fitness-tracking#OutdoorWalking"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Location -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Location">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#GeospatialMeasurement"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#Point"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <owl:disjointWith rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Route"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Metric -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Metric"/>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#OutdoorCycling -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#OutdoorCycling">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Cycling"/>
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#OutdoorTraining"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#OutdoorRunning -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#OutdoorRunning">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#OutdoorTraining"/>
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Running"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#OutdoorTraining -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#OutdoorTraining">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Training"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#OutdoorWalking -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#OutdoorWalking">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#OutdoorTraining"/>
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Walking"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Route -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Route">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#GeospatialMeasurement"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Running -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Running">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#CardiovascularTraining"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#StepCount -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#StepCount">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#TrainingMetric"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Training -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Training"/>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#TrainingMetric -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#TrainingMetric">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#Metric"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#TreadmillRunning -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#TreadmillRunning">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#IndoorRunning"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#TreadmillWalking -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#TreadmillWalking">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#IndoorWalking"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Walking -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Walking">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#CardiovascularTraining"/>
    </owl:Class>
    


    <!-- http://drozdowicz.net/onto/fitness-tracking#Weight -->

    <owl:Class rdf:about="http://drozdowicz.net/onto/fitness-tracking#Weight">
        <rdfs:subClassOf rdf:resource="http://drozdowicz.net/onto/fitness-tracking#HealthMeasurement"/>
    </owl:Class>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#Point -->

    <owl:Class rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#Point">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
        <rdfs:comment> 
Uniquely identified by lat/long/alt. i.e.

spaciallyIntersects(P1, P2) :- lat(P1, LAT), long(P1, LONG), alt(P1, ALT),
  lat(P2, LAT), long(P2, LONG), alt(P2, ALT).

sameThing(P1, P2) :- type(P1, Point), type(P2, Point), spaciallyIntersects(P1, P2).
  </rdfs:comment>
        <rdfs:comment>A point, typically described using a coordinate system relative to Earth, such as WGS84.
  </rdfs:comment>
        <rdfs:label>point</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing -->

    <owl:Class rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing">
        <rdfs:comment>Anything with spatial extent, i.e. size, shape, or position.
 e.g. people, places, bowling balls, as well as abstract areas like cubes.
</rdfs:comment>
        <rdfs:label>SpatialThing</rdfs:label>
    </owl:Class>
    


