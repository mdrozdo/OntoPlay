<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/sxacml/access-control-action#"
     xml:base="https://w3id.org/sxacml/access-control-action"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:org="http://www.w3.org/ns/org#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://w3id.org/sxacml/access-control-action">
        <owl:imports rdf:resource="https://w3id.org/sxacml/access-control"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/source -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/source"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/sxacml/access-control-action#DataAction -->

    <owl:Class rdf:about="https://w3id.org/sxacml/access-control-action#DataAction">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sxacml/access-control#Action"/>
    </owl:Class>
    


    <!-- https://w3id.org/sxacml/access-control-action#PhysicalAction -->

    <owl:Class rdf:about="https://w3id.org/sxacml/access-control-action#PhysicalAction">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sxacml/access-control#Action"/>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/sxacml/access-control-action#Abort -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Abort">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act to &quot;aborted&quot;, i.e., terminated prior to the originally intended completion.</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Activate -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Activate">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act to &quot;active&quot;, i.e., so it can be performed or is being performed, for the first time. (Contrast with REACTIVATE.)</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Annotate -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Annotate">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Add commentary, explanatory notes, critical notes or similar content to an object.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Anonymize -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Anonymize">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Remove any information that could result in identifying the information subject.  [From ANONY concept in HL7 ActCode code system, OID: 2.16.840.1.113883.5.4] </dc:description>
        <dc:source>HL7 v3 ObligationPolicy value set</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Append -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Append">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Fundamental operation in an Information System (IS) that results only in the addition of information to an object already in existence.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Archive -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Archive">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Move (the content of) an object to long term storage.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Backup -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Backup">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Produce another object with the same content as the original for potential recovery, i.e., create a spare copy.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Cancel -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Cancel">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act to &quot;cancelled&quot;, i.e., abandoned before activation.</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Close -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Close">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#PhysicalAction"/>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Complete -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Complete">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act to &quot;completed&quot;, i.e., terminated normally after all of its constituents have been performed.</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Convert -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Convert">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Derive another object with the same content in a different form (different data model, different representation, and/or different format).</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Copy -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Copy">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Produce another online object with the same content as the original.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Create -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Create">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Fundamental operation in an Information System (IS) that results only in the act of bringing an object into existence.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Decrypt -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Decrypt">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Render information readable by algorithmically transforming ciphertext into plaintext. [Derived from ENCRYPT concept in HL7 ActCode code system, OID: 2.16.840.1.113883.5.4]</dc:description>
        <dc:source>HL7 v3 ObligationPolicy value set</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Deduplicate -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Deduplicate">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Remove repetition/copies of data.  [March 12, 2013 HL7 Security Work Group teleconference]</dc:description>
        <dc:source>teleconference</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Deidentify -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Deidentify">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Strip information of data that would allow the identification of the source of the information or the information subject.  [From DEID concept in HL7 ActCode code system, OID: 2.16.840.1.113883.5.4]</dc:description>
        <dc:source>HL7 v3 ObligationPolicy value set</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Delete -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Delete">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Fundamental operation in an Information System (IS) that results only in the removal of information about an object from memory or storage.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Derive -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Derive">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Make another object with content based on but different from that of an existing object.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Encrypt -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Encrypt">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Render information unreadable by algorithmically transforming plaintext into ciphertext. [From ENCRYPT concept in HL7 ActCode code system, OID: 2.16.840.1.113883.5.4]</dc:description>
        <dc:source>HL7 v3 ObligationPolicy value set</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Enter -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Enter">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#PhysicalAction"/>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Excerpt -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Excerpt">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Derive another object which includes part but not all of the original content.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Execute -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Execute">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Fundamental operation in an IS that results only in initiating performance of a single or set of programs (i.e., software objects).</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Export -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Export">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Reproduce an object (or a portion thereof) so that the data leaves the control of the security subsystem.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Forward -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Forward">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Communicate (the content of) an object to another covered entity.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Hold -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Hold">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act to &quot;held&quot;</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Identify -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Identify">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Add information to data that would allow the identification of the source of the information or the information subject.  [Derived from DEID concept in HL7 ActCode code system, OID: 2.16.840.1.113883.5.4] </dc:description>
        <dc:source>[Deidentify]</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Import -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Import">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Reproduce an object (or a portion thereof) so that the data enters the control of the security subsystem.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Jump -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Jump">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act to a normal state.</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Leave -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Leave">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#PhysicalAction"/>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Load -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Load">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#PhysicalAction"/>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Lock -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Lock">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#PhysicalAction"/>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Mask -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Mask">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Render information unreadable and unusable by algorithmically transforming plaintext into ciphertext. User may be provided a key to decrypt per license or &quot;shared secret&quot;. [From MASK concept in HL7 ActCode code system, OID: 2.16.840.1.113883.5.4]</dc:description>
        <dc:source>HL7 v3 ObligationPolicy value set</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Modify_status -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Modify_status">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act.</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Move -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Move">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Relocate (the content of) an object.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Nullify -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Nullify">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act to &quot;nullified&quot;, i.e., treat as though it never existed.</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Obsolete -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Obsolete">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act to &quot;obsolete&quot; when it has been replaced by a new instance.</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Open -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Open">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#PhysicalAction"/>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Print -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Print">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Render an object in printed form (typically hardcopy).</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Pseudonymize -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Pseudonymize">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Strip information of data that would allow the identification of the source of the information or the information subject. Custodian may retain a key to relink data necessary to reidentify the information subject. [From PSEUD concept in HL7 ActCode code system, OID: 2.16.840.1.113883.5.4] </dc:description>
        <dc:source>HL7 v3 ObligationPolicy value set</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Purge -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Purge">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Operation that results in the permanent, unrecoverable removal of information about an object from memory or storage (e.g. by multiple overwrites with a series of random bits).</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Reactivate -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Reactivate">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of a formerly active object representing an Act to &quot;active&quot;, i.e., so it can again be performed or is being performed. (Contrast with ACTIVATE.)</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Read -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Read">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Fundamental operation in an Information System (IS) that results only in the flow of information about an object to a subject.</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Redact -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Redact">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Remove information, which is not authorized to be accessed, used, or disclosed from records made available to otherwise authorized users. [From REDACT concept in HL7 ActCode code system, OID: 2.16.840.1.113883.5.4]</dc:description>
        <dc:source>HL7 v3 ObligationPolicy value set</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Reidentify -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Reidentify">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description>Restore information to data that would allow the identification of the source of the information or the information subject.  [Derived from DEID concept in HL7 ActCode code system, OID: 2.16.840.1.113883.5.4]</dc:description>
        <dc:source>[Deidentify]</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Release -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Release">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Change the status of an object representing an Act so it is no longer &quot;held&quot;, i.e., allow action to occur.</dc:description>
        <dc:source>HL7 DataOperation Vocabulary</dc:source>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/sxacml/access-control-action#Replace -->

    <owl:NamedIndividual rdf:about="https://w3id.org/sxacml/access-control-action#Replace">
        <rdf:type rdf:resource="https://w3id.org/sxacml/access-control-action#DataAction"/>
        <dc:description xml:lang="en">Replace an object with another object. (The replaced object becomes obsolete in the process.)</dc:description>
        <dc:source>HL7 RBAC</dc:source>
    </owl:NamedIndividual>
    


